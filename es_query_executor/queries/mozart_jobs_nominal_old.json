{
  "query": {
    "bool": {
      "must": [
        {
          "bool": {
            "should": [
              {
                "match": {
                  "status": "job-completed"
                }
              },
              {
                "match": {
                  "status": "job-revoked"
                }
              },
              {
                "match": {
                  "status": "job-deduped"
                }
              },
              {
                "match": {
                  "status": "job-failed"
                }
              }
            ]
          }
        },
        {
          "range": {
            "@timestamp": {
              "lt": "now-14d/d"
            }
          }
        }
      ]
    }
  }
}